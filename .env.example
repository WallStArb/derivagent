# Derivagent Environment Variables Template
# Copy this to .env and add your real keys
# NEVER commit the real .env file


# Application Configuration
ENVIRONMENT=development
LOG_LEVEL=INFO
FASTAPI_DEBUG=true

# Security
JWT_SECRET_KEY=your-jwt-secret-key
ENCRYPTION_KEY=your-encryption-key

# Broker API Keys (add as needed)
TASTYTRADE_API_KEY=your-tastytrade-key
IBKR_API_KEY=your-ibkr-key
SCHWAB_API_KEY=your-schwab-key

# External Data Sources
POLYGON_API_KEY=your-polygon-key
ALPHA_VANTAGE_API_KEY=your-alpha-vantage-key

# Monitoring & Analytics
POSTHOG_API_KEY=your-posthog-key
SENTRY_DSN=your-sentry-dsn
